#include <stdio.h>

int main() {
    int file[50];
    int n, i, record;

    printf("Enter number of records in the file: ");
    scanf("%d", &n);

    // Store records sequentially
    printf("Enter record values:\n");
    for (i = 0; i < n; i++) {
        scanf("%d", &file[i]);
    }

    printf("Enter record number to access (1 to %d): ", n);
    scanf("%d", &record);

    if (record < 1 || record > n) {
        printf("Invalid record number\n");
        return 0;
    }

    printf("\nAccessing records sequentially:\n");

    // Sequential access simulation
    for (i = 0; i < record; i++) {
        printf("Reading record %d : %d\n", i + 1, file[i]);
    }

    printf("\nRecord %d accessed successfully.\n", record);

    return 0;
}
